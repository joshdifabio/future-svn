{
    "name": "joshdifabio/svn-client",
    "description": "SVN client for PHP using futures and promises",
    "keywords": ["svn", "subversion", "client", "promise", "react"],
    "license": "MIT",
    "authors": [
        {
            "name": "Joshua Di Fabio",
            "email": "joshdifabio@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "joshdifabio/future-process": "dev-master",
        "react/promise": "~1.0|~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.5|~4.0",
        "satooshi/php-coveralls": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "SubversionClient\\": "src"
       }
    },
    "autoload-dev": {
        "psr-4": {
            "SubversionClient\\": "tests"
        }
    }
}
